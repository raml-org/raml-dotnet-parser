              }
            },
          "Id": { "type": "integer"},
          "Title": { "type": "string"},
          "Tracks":
            {
              "type": "array",
              "items": 
              {
                "type": "object",
                "properties": 
                {
                "Id": { "type": "integer"},
                "Name": { "type": "string"},
                "Album":
                  { "$ref": "Album" },
                "MediaType":
                  { 
                    "type": "object",
                    "id": "MediaType",
                    "properties": {
