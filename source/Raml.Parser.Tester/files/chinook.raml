                "Milliseconds": { "type": "integer"},
                "Bytes": { "type": "integer"},
                "UnitPrice": { "type": "number"}
                }
              }
            }
        }
      }
      
  - Invoice: |
      { 
        "$schema": "http://json-schema.org/draft-03/schema",
        "type": "object",
        "id": "Invoice",
        "properties": {
          "Id": { "type": "integer"},
          "Customer":
            { 
              "type": "object",
              "id": "Customer",
              "properties": {
                "Id": { "type": "integer"},
                "Company": { "type": "string"},
                "SupportRepresentant":
                  { 
                    "type": "object",
